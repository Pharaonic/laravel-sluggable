{
    "name": "pharaonic/laravel-sluggable",
    "description": "Simplest Sluggable for Laravel Eloquent/Model",
    "keywords": [
        "php",
        "laravel",
        "slug",
        "sluggable",
        "eloquent",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny",
            "email": "raggigroup@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "laravel/framework": "^12.0",
        "pharaonic/php-slugify": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Sluggable\\SluggableServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Sluggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pharaonic\\Laravel\\Sluggable\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
